            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".Courses" />
        <!-- <activity android:name=".SplashScreen"></activity> -->
        <activity android:name=".Attendance" />
        <activity android:name=".CourseDashboard" />
